{
  "private": true,
  "scripts": {
    "convert-svg": "svgr -d src/static/svg-components public/static/svgs --ext tsx",
    "build": "cross-env ANALYZE=1 umi build",
    "start": "node server.js",
    "dev": "cross-env UMI_UI=none NODE_ENV=development concurrently \"umi dev\" \"nodemon server.js\"",
    "buildAndCopy": "cross-env ANALYZE=1 umi build && node cp.js",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@quentin-sommer/react-useragent": "^3.1.0",
    "@types/classnames": "^2.2.10",
    "@umijs/preset-react": "^1.5.17",
    "@umijs/test": "^3.2.9",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "classnames": "^2.2.6",
    "highcharts": "^8.2.2",
    "highcharts-react-official": "^3.0.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "opensea-js": "^1.1.11",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.9",
    "use-wallet": "^0.8.1",
    "web3": "^1.3.5"
  },
  "devDependencies": {
    "@svgr/cli": "^5.4.0",
    "@types/lodash": "^4.14.168",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "got": "^11.5.0",
    "http-proxy-middleware": "^1.0.5",
    "koa": "^2.13.0",
    "koa-compress": "^5.0.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "koa-websocket": "^6.0.0",
    "koa2-proxy-middleware": "^0.0.4",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
